    Private Sub EnterExamQuestionsToolStripMenuItem_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles EnterExamQuestionsToolStripMenuItem.Click
        frmAdminLogin.Show()
        Me.Hide()
    End Sub


    Private Sub HomeToolStripMenuItem_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles HomeToolStripMenuItem.Click
        Form1.Show()
        Me.Hide()
    End Sub

    
    Private Sub frmAddQuestions_Load(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles MyBase.Load
        Label14.Text = frmStudentLogin.NuNameTB.Text
    End Sub

    Private Sub ResultToolStripMenuItem_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles ResultToolStripMenuItem.Click
        NuNameLB = Label14
        frmResult.Show()
        Me.Hide()
    End Sub

    Private Sub AnswerQuestionsToolStripMenuItem_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles AnswerQuestionsToolStripMenuItem.Click
